<h1>My Cleanups</h1>


<table class= "hover">
  <thead>
    <tr>
      <th width="200">Date</th>
      <th>Cleanup Event</th>   
    </tr>
  </thead>
  <tbody>
  <% @cleanups.each do |cleanup| %>
    <tr>
      <td><%= cleanup.date.strftime("%m-%d-%Y") %></td> 
      <td><%= link_to cleanup.title, cleanup %></td>
    </tr>   
    <% end %>
  </tbody>
</table>




